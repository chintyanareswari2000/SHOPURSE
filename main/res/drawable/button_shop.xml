<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="300dp" />
            <stroke android:color="#235C2F" />
            <gradient android:angle="-90" android:startColor="#235C2F" android:endColor="#5eec7c"  />
        </shape>
    </item>

    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="300dp" />
            <stroke android:color="#5eec7c" />
            <gradient android:angle="-90" android:startColor="#5eec7c" android:endColor="#235C2F" />
        </shape>
    </item>
</selector>